@charset "utf-8";
/* CSS Document */

@import url("reset.css");

#Wrapper {width:auto; height:auto; margin:0px; padding:0px; background-color:#b298d2;font-family:Arial, "新細明體";}

#Header {width:auto; height:41px; margin:0px ; padding:0px; border-bottom:5px solid #ccc}

.headbox {width:1000px; height:41px; margin:0px auto; padding:0px;background:url(../images/bg-top.jpg) no-repeat;}

.bheadbox {width:1000px; height:20px; margin:0px auto; padding:0px;background:url(../images/bg-top.jpg) no-repeat;}

#Menu {width:1000px; height:60px; margin:0px ; padding:0px; background-color:#fff;border-bottom:2px solid #88abda}

.menubox {width:930px; height:60px; margin:0px auto; padding:0px;  }

#Container {width:auto; height:auto; margin:0px ; padding:0px; background-color:#fff }

#Content {width:1000px; height:auto; margin:0px auto; padding:0px; }




.clearbox { clear:both; }

/*---------------------------------------------------上方區-----------------------------------------------------*/

/*================位置================*/

.linebox{ width:300px; margin:8px 0px 0px 45px; padding:0px;float:left; font-size:15px;}

.linebox a{ color:#000; text-decoration:none;}
.linebox a:hover{ color:#FFF; }

.linebox img{ width:9px; height:7px; margin:0px 12px;}

.logout {width:50px; margin:8px 20px 0px 0px; padding:0px;float:right; font-size:15px;}
.logout a{ color:#000; text-decoration:none;}
.logout a:hover{ color:#FFF; }

